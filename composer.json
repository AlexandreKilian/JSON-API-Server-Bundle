{
  "name": "enm/json-api-server-bundle",
  "description": "Symfony integration of enm/json-api-server",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Server\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Server\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.0",
    "symfony/dependency-injection": "^2.7|^3.0",
    "symfony/config": "^2.7|^3.0",
    "symfony/http-kernel": "^2.7|^3.0",
    "symfony/event-dispatcher": "^2.7|^3.0",
    "enm/json-api-server": "^2.2",
    "symfony/psr-http-message-bridge": "^1.0",
    "guzzlehttp/psr7": "^1.0"
  },
  "require-dev": {
    "symfony/symfony": "^2.7|^3.0",
    "phpunit/phpunit": "^6.1"
  },
  "suggest": {
    "symfony/proxy-manager-bridge": "Required to use lazy services."
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "config": {
    "platform": {
      "php": "7.0"
    }
  },
  "minimum-stability": "stable"
}
